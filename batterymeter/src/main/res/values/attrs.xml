<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BatteryMeterView">
        <attr name="battery_theme" format="enum">
            <enum name="rounded" value="0" />
            <enum name="sharp" value="1" />
        </attr>

        <attr name="battery_chargeLevel" format="integer" />
        <attr name="battery_criticalChargeLevel" format="integer" />
        <attr name="battery_isCharging" format="boolean" />
    </declare-styleable>
</resources>